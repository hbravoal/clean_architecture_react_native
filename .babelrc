{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "domain": "./src/domain",
          "infrastructure": "./src/infrastructure",
          "application": "./src/application",
          "presentation": "./src/presentation"
        }
      }
    ]
  ]
}
